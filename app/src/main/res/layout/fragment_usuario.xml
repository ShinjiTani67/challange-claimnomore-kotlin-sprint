<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".UsuarioFragment"
    android:id="@+id/frameLayout5">

    <TextView
        android:id="@+id/usuario_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:fontFamily="@font/jost"
        android:text="@string/usuario_text"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.183"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="58dp"
        tools:layout_conversion_absoluteWidth="127dp" />

    <Button
        android:id="@+id/notafiscal_btn"
        android:layout_width="298dp"
        android:layout_height="45dp"
        android:layout_marginTop="156dp"
        android:text="@string/notafiscal_button_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.46"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="45dp"
        tools:layout_conversion_absoluteWidth="298dp" />

    <Button
        android:id="@+id/sinistrofalso_btn"
        android:layout_width="298dp"
        android:layout_height="45dp"
        android:layout_marginTop="248dp"
        android:text="@string/sinistrofalso_button_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.46"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="49dp"
        tools:layout_conversion_absoluteWidth="296dp" />

    <Button
        android:id="@+id/dentistasparceiros_btn"
        android:layout_width="294dp"
        android:layout_height="46dp"
        android:layout_marginTop="416dp"
        android:text="@string/dentistasparceiros_button_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="299dp" />

    <Button
        android:id="@+id/sinistro_btn"
        android:layout_width="298dp"
        android:layout_height="45dp"
        android:layout_marginTop="332dp"
        android:text="@string/sinistro_button_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.46"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="45dp"
        tools:layout_conversion_absoluteWidth="295dp" />

</androidx.constraintlayout.widget.ConstraintLayout>